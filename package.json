{
  "name": "@adobe/helix-perf",
  "version": "1.1.8",
  "description": "Helix Performance",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "nyc mocha -i -g Post-Deployment --exit",
    "test-ci": "npm run lint && nyc --reporter=text --reporter=lcov --check-coverage --branches 90 --statements 90 --lines 90 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g Post-Deployment  --exit && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g Post-Deployment  --exit",
    "semantic-release": "semantic-release",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci",
    "lint": "./node_modules/.bin/eslint .",
    "build": "wsk-builder -v --entryFile index.js",
    "deploy": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml"
  },
  "wsk": {
    "name": "helix-services-private/perf@${version}",
    "linksPackage": "helix-services"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-perf"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-perf/issues"
  },
  "homepage": "https://github.com/adobe/helix-perf#readme",
  "dependencies": {
    "@adobe/fastly-native-promises": "1.14.0",
    "@adobe/helix-status": "5.1.0",
    "@adobe/openwhisk-action-utils": "2.3.4",
    "calibre": "2.3.2",
    "epsagon": "1.37.0",
    "uri-js": "4.2.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@adobe/helix-shared": "3.0.3",
    "@adobe/openwhisk-action-builder": "2.1.1",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.3.0",
    "@semantic-release/exec": "3.3.7",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/github": "5.5.0",
    "@semantic-release/release-notes-generator": "7.3.0",
    "ajv": "6.10.2",
    "codecov": "3.6.1",
    "eslint": "6.5.1",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "istanbul": "1.1.0-alpha.1",
    "junit-report-builder": "1.3.3",
    "mocha": "6.2.1",
    "mocha-junit-reporter": "1.23.1",
    "mocha-parallel-tests": "2.2.2",
    "nyc": "14.1.1",
    "proxyquire": "2.1.3",
    "semantic-release": "15.13.24",
    "sinon": "7.5.0"
  },
  "statuspage": {
    "name": "Helix Performance",
    "group": "Development"
  },
  "newrelic": {
    "group_policy": "Development Repeated Failure"
  }
}
