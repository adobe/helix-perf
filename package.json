{
  "name": "@adobe/helix-perf",
  "version": "1.1.56",
  "description": "Helix Performance",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "nyc mocha -i -g Post-Deployment --exit",
    "test-release": "semantic-release --dry-run",
    "test-ci": "npm run lint && nyc --reporter=text --reporter=lcov --check-coverage --branches 90 --statements 90 --lines 90 mocha --reporter xunit --reporter-options output=./junit/test-results.xml -i -g Post-Deployment  --exit && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g Post-Deployment  --exit",
    "semantic-release": "semantic-release",
    "deploy-sequences": "wsk-builder --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci",
    "lint": "./node_modules/.bin/eslint .",
    "build": "wsk-builder -v",
    "deploy": "wsk-builder -v --deploy --test=/_status_check/pingdom.xml"
  },
  "wsk": {
    "name": "helix-services/perf@${version}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-perf"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-perf/issues"
  },
  "homepage": "https://github.com/adobe/helix-perf#readme",
  "dependencies": {
    "@adobe/fastly-native-promises": "1.15.1",
    "@adobe/helix-epsagon": "1.2.0",
    "@adobe/helix-status": "7.1.3",
    "@adobe/openwhisk-action-logger": "2.2.0",
    "@adobe/openwhisk-action-utils": "4.2.2",
    "calibre": "3.1.0",
    "uri-js": "4.2.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.0",
    "@adobe/helix-ops": "1.10.0",
    "@adobe/helix-shared": "7.0.1",
    "@adobe/openwhisk-action-builder": "2.10.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "codecov": "3.6.5",
    "eslint": "6.8.0",
    "eslint-plugin-header": "3.0.0",
    "eslint-plugin-import": "2.20.2",
    "junit-report-builder": "1.3.3",
    "mocha": "7.1.1",
    "mocha-junit-reporter": "1.23.3",
    "mocha-parallel-tests": "2.3.0",
    "nyc": "15.0.1",
    "proxyquire": "2.1.3",
    "semantic-release": "17.0.6",
    "sinon": "9.0.2"
  }
}
